package org.com.jack.kitchener.extended;

public class Rental {
	
	protected int days;
	protected int dist;
	protected double costPerKm = 1;
	
	 void setDays(int days){
		this.days = days;
	}
	
	double getDist(){
		return dist;
	}
	
	void setDist(int dist){
		this.dist = dist;
	}
	

	 void setCost(double cost){
		this.costPerKm = cost;
	}

}
